<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AutoBannerView">
        <attr name="dotGravity" format="enum">
            <enum name="left" value="1" />
            <enum name="center" value="2" />
            <enum name="right" value="3"/>
        </attr>
        <attr name="waitMilliSecond" format="integer"/>
        <attr name="dotMargin" format="dimension" />
        <attr name="dotVisible" format="enum">
            <enum name="visible" value="0"/>
            <enum name="invisible" value="4"/>
            <enum name="gone" value="8"/>
        </attr>
    </declare-styleable>
</resources>